---
import ExperimentLoss from "./ExperimentLoss.astro";
import {
    tubingen_conv4,
    tubingen_conv5,
    tubingen_conv2,
    tubingen_conv1,
    tubingen_conv3,
    fallingwater_conv1,
    fallingwater_conv5,
} from "./data";

const data = [
    { name: "Tubingen Conv1", values: tubingen_conv1 },
    { name: "Tubingen Conv2", values: tubingen_conv2 },
    { name: "Tubingen Conv3", values: tubingen_conv3 },
    { name: "Tubingen Conv4", values: tubingen_conv4 },
    { name: "Tubingen Conv5", values: tubingen_conv5 },
    { name: "Falling Water Conv1", values: fallingwater_conv1 },
    { name: "Falling Water Conv5", values: fallingwater_conv5 },
];
---

<ExperimentLoss data={data} />
